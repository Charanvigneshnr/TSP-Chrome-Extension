{
    "manifest_version": 2,
    "name": "TSP-Chrome-Extension",
    "version": "1.0",
    "description": "A Chrome extension for optimizing routes",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "permissions": [
      "geolocation",
      "tabs",
      "activeTab",
      "storage",
      "https://maps.googleapis.com/*"
    ],
    "browser_action": {
      "default_title": "TSP Extension",
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://maps.googleapis.com/ https://*.googleapis.com; object-src 'self'",
    "key": "AIzaSyCGX4X5Xz4sU5s84JmY5hy4knhZ6QKdp3s"
}